{
  "name": "@starter/ts-sdk",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": ["dist", "idl"],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./idl/*": {
      "types": "./dist/idl/*.d.ts",
      "import": "./dist/idl/*.js"
    },
    "./idl": {
      "types": "./dist/idl/index.d.ts",
      "import": "./dist/idl/index.js"
    }
  },
  "typesVersions": {
    "*": {
      "idl": ["dist/idl/index.d.ts"],
      "idl/*": ["dist/idl/*.d.ts"]
    }
  },
  "scripts": {
    "build": "node ../../scripts/sync-idl.mjs copy-src && tsc -p . && node ../../scripts/sync-idl.mjs copy-dist",
    "prepublishOnly": "pnpm build"
  },
  "devDependencies": {
    "typescript": "5.6.2"
  }
}
